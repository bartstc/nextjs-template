{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "importOrder": [
    "^react",
    "^next",
    "^(?!(hooks|types|utils|shared|modules|[./])).+",
    "^hooks",
    "^types",
    "^utils",
    "^shared",
    "modules",
    "^[./]"
  ],
  "importOrderSeparation": true
}
